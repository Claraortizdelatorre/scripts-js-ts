{
  "compilerOptions":{
    "lib": ["DOM", "DOM.Iterable", "ESNext"],
    "target": "es5",
    "module": "esnext",
    "watch": true,
    "strict": true,
    "noImplicitAny": false,
    "allowJs": true,
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "outDir": "transpiled"
  },
  "compileOnSave": true,
  "exclude": ["node_modules"],
  "include": ["scripts"]
 }